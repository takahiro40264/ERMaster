<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<appender
		name="STDOUT"
		class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%date{yyyy-MM-dd HH:mm:ss.SSS} [%-5le] %m - %C.%M:%L%n
			</pattern>
		</encoder>
	</appender>
	<logger
		name="ermaster"
		level="INFO"
		additivity="false">
		<appender-ref ref="STDOUT" />
	</logger>
</configuration>